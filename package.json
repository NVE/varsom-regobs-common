{
  "name": "varsom-regobs-common",
  "version": "1.0.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build core && ng build regobs-api && ng build registration && npm run copy-registration-lib-assets && ng build",
    "copy-registration-lib-assets": "copyfiles -f packages/registration/src/assets/kdvelements/*.json dist/varsom-regobs-common/registration/assets/kdvelements",
    "test": "ng test --watch=false --browsers ChromeHeadless --reporters mocha --progress=true",
    "test:ci": "ng test --watch=false --browsers ChromeHeadlessCI --reporters junit --code-coverage --progress=false",
    "test:watch": "ng test --watch=true --browsers ChromeHeadless --reporters mocha --progress=true",
    "lint": "ng lint --format stylish",
    "lint:ci": "ng lint --format vso",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.11",
    "@angular/common": "^8.2.11",
    "@angular/compiler": "^8.2.11",
    "@angular/core": "^8.2.11",
    "@angular/forms": "^8.2.11",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.2.11",
    "@angular/platform-browser-dynamic": "^8.2.11",
    "@angular/router": "^8.2.11",
    "@nano-sql/core": "^2.3.7",
    "core-js": "^3.3.2",
    "moment": "^2.24.0",
    "ngx-logger": "^4.0.7",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.12",
    "@angular-devkit/build-ng-packagr": "~0.803.12",
    "@angular/cli": "~8.3.12",
    "@angular/compiler-cli": "^8.2.11",
    "@angular/language-service": "^8.2.11",
    "@types/jasmine": "~3.4.4",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.7",
    "codelyzer": "~5.1.2",
    "copyfiles": "^2.1.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^5.7.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.4.1",
    "tsickle": ">=0.37.0",
    "tslib": "^1.10.0",
    "tslint": "~5.20.0",
    "typescript": "^3.5.3"
  }
}