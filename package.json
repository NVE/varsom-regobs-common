{
  "name": "varsom-regobs-common",
  "version": "1.0.11",
  "scripts": {
    "ng": "ng",
    "eslint": "eslint",
    "start": "ng serve --open",
    "build": "ng build core && ng build regobs-api && ng build registration && npm run copy-registration-lib-assets && ng build",
    "copy-registration-lib-assets": "npm run cp:kdv-elements && npm run cp:helptexts",
    "cp:kdv-elements": "copyfiles -f packages/registration/src/assets/kdvelements/*.json dist/varsom-regobs-common/registration/assets/kdvelements",
    "cp:helptexts": "copyfiles -f packages/registration/src/assets/helptexts/*.json dist/varsom-regobs-common/registration/assets/helptexts",
    "test": "ng test --watch=false --browsers ChromeHeadless --reporters mocha --progress=true",
    "test:ci": "ng test --watch=false --browsers ChromeHeadlessCI --reporters junit --code-coverage --progress=false",
    "test:watch": "ng test --watch=true --browsers ChromeHeadless --reporters mocha --progress=true",
    "lint": "eslint packages/*/src/**.ts",
    "lint:ci": "eslint packages/*/src/**.ts --format vso",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@nano-sql/core": "^2.3.7",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "clone-deep": "^4.0.1",
    "core-js": "^3.6.4",
    "eslint-formatter-vso": "^0.1.0",
    "moment": "^2.24.0",
    "ngx-connectivity": "0.0.5",
    "ngx-logger": "^4.1.1",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.23",
    "@angular-devkit/build-ng-packagr": "~0.803.23",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "~3.5.1",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "codelyzer": "~5.2.1",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^21.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.1",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^5.7.1",
    "ngx-translate-testing": "^3.0.0",
    "prettier": "^1.19.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.6.2",
    "tsickle": "^0.37.1",
    "tslib": "^1.10.0",
    "tslint": "~6.0.0",
    "typescript": "^3.5.3"
  }
}
